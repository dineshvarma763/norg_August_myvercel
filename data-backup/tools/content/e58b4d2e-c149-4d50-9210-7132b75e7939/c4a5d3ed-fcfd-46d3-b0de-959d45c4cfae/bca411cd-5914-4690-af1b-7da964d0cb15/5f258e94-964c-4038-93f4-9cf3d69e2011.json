{"_currentVersionState": {"$invariant": "PUBLISHED"}, "name": {"$invariant": "Toro"}, "_updateDate": {"$invariant": "2023-08-18T01:08:33.317Z"}, "_hasChildren": true, "_level": 4, "_createDate": "2023-06-20T08:07:01.573Z", "_id": "5f258e94-964c-4038-93f4-9cf3d69e2011", "_links": {"self": {"href": "https://api.umbraco.io/content/5f258e94-964c-4038-93f4-9cf3d69e2011"}, "root": {"href": "https://api.umbraco.io/content"}, "children": {"href": "https://api.umbraco.io/content/5f258e94-964c-4038-93f4-9cf3d69e2011/children"}, "publish": {"href": "https://api.umbraco.io/content/5f258e94-964c-4038-93f4-9cf3d69e2011/publish"}, "unpublish": {"href": "https://api.umbraco.io/content/5f258e94-964c-4038-93f4-9cf3d69e2011/unpublish"}, "contenttype": {"href": "https://api.umbraco.io/content/type/productPage"}}, "contentTypeAlias": "productPage", "parentId": "bca411cd-5914-4690-af1b-7da964d0cb15", "sortOrder": 3, "showInNavigation": {"$invariant": "1"}, "externalLink": {"$invariant": []}, "umbracoUrlName": {"$invariant": ""}, "superAlias": {"$invariant": "/au/commercial/rolling-door-openers/toro"}, "prefetch": {"$invariant": "1"}, "source": {"$invariant": ""}, "destination": {"$invariant": ""}, "isPermanent": {"$invariant": "1"}, "sEOTitle": {"$invariant": ""}, "sEODescription": {"$invariant": "Toro Opener (GDO-10) by Automatic Technology offers robust commercial roller door tech with superior pulling power and built-in battery backup. Perfect for high-duty doors up to 28m\u00b2."}, "showInSitemap": {"$invariant": "1"}, "ogImage": {"$invariant": []}, "ogDescription": {"$invariant": "Toro Opener (GDO-10) by Automatic Technology offers robust commercial roller door tech with superior pulling power and built-in battery backup. Perfect for high-duty doors up to 28m\u00b2."}, "canonicalURLContentItem": {"$invariant": ""}, "canonicalURLAbsolute": {"$invariant": ""}, "structuredData": {"$invariant": ""}, "noIndexPage": {"$invariant": "0"}, "alternateMultiURLs": {"$invariant": []}, "heading":